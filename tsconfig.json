{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2021",
    "sourceMap": true,
    "esModuleInterop": true,
    "typeRoots": [
      "./node_modules/@types",
      "./src/types/express.d.ts"
    ],
    "baseUrl": "./src",
    "paths": {
      "$util/*": [
        "util/*"
      ],
      "$types/*": [
        "types/*"
      ],
      "$gcp/*": [
        "gcp/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
